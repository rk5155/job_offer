<html>
  <head>
    <style>
      @media print {
        body, page {
          margin: 0;
          box-shadow: 0;
        }
      }
      html {
        font-size: 62.5%;
      }
      body {
        font-size: 8px;
        /* font-size: 1rem; */
      }
      page {
        background: white;
        display: block;
        margin: 0 auto;
        width: 100%;
        height: 100%;
      }
      page[size="A4"][layout="portrait"] {
        width: 100%;
        height: 100%;
      }
      tbody {
        font-size: 12px;
      }
      td {
        padding: 0 5px;
      }
      .bd-t-s {
        border-top: 1px solid;
      }
      .bd-r-s {
        border-right: 1px solid;
      }
      .bd-b-s {
        border-bottom: 1px solid;
      }
      .bd-l-s {
        border-left: 1px solid;
      }
      .bd-t-dt {
        border-top: 1px dotted;
      }
      .bd-r-dt {
        border-right: 1px dotted;
      }
      .bd-b-dt {
        border-bottom: 1px dotted;
      }
      .bd-l-dt {
        border-left: 1px dotted;
      }
      .bd-t-db {
        border-top: 4px double;
      }
      .center {
        text-align: center;
      }
      html {
        font-family: 'IPAexゴシック', 'IPA Pゴシック', 'ヒラギノ角ゴ ProN W3', 'Hiragino Kaku Gothic ProN', 'メイリオ', Meiryo, 'MS Pゴシック', sans-serif;
      }
    </style>

    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
  </head>
  <body>
    <page size="A3" layout="portrait" style="padding: 60px; box-sizing: border-box">
      <table border-collapse="collapse" cellspacing="0" cellpadding="0" style="table-layout:fixed; width:0">
        <thead>
          <tr style="height: 0px">
            <th style="width:70px"></th>
            <th style="width:45px"></th>
            <th style="width:155px"></th>
            <th style="width:70px"></th>
            <th style="width:70px"></th>
            <th style="width:130px"></th>
            <th style="width:60px"></th>
            <th style="width:70px"></th>
            <th style="width:45px"></th>
            <th style="width:425px"></th>
          </tr>
        </thead>
        <tbody>
          <tr style="height:33.5px">
            <td style="font-size:2rem; line-height:2rem; text-align:center" colspan="2">履 歴 書</td>
            <td></td>
            <td></td>
            <td></td>
            <td style="text-align:right; font-size: 10px;"><%= @updated_at_year %>年 <%= @updated_at_month %>月 <%= @updated_at_day %>日 現在</td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-s bd-l-s">フリガナ</td>
            <td class="bd-t-s bd-l-s" colspan="3"><%= @user_devise.furigane %> <%= @user_devise.furigane2 %></td>
            <td class="bd-t-s bd-l-dt center" rowspan="3"><%= @user_devise.sex %></td>
            <td class="bd-t-s bd-r-s bd-l-s center" rowspan="4">（写真を貼る位置）</td>
            <td></td>
            <td class="bd-t-s bd-l-s center">年</td>
            <td class="bd-t-s bd-l-dt center">月</td>
            <td class="bd-t-s bd-r-s bd-l-dt center">職歴</td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s" rowspan="2">氏 名</td>
            <td class="bd-t-dt" colspan="3" rowspan="2"><%= @user_devise.name %> <%= @user_devise.name2 %></td>
            <td></td>
            <td class="bd-t-db bd-l-s center"><%= @period_year_from1 %></td>
            <td class="bd-t-db bd-l-dt center"><%= @period_month_from1 %></td>
            <td class="bd-t-db bd-r-s bd-l-dt center"><%= @company_name1 %></td>
          </tr>
          <tr style="height:33.5px">
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @period_year_from2 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @period_month_from2 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center"><%= @company_name2 %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-s bd-l-s">生年月日</td>
            <td class="bd-t-s bd-l-s" colspan="4"><%= @user_devise.birthday %>年 <%= @user_devise.month %>月 <%= @user_devise.day %>日生 (満<%= @user_devise.age %>歳)</td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @period_year_from3 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @period_month_from3 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center"><%= @company_name3 %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-s bd-l-s">フリガナ</td>
            <td class="bd-t-s bd-r-s bd-l-s" colspan="5">トウキョウトミナトクシバコウエン4-2-8</td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @user_devise.career_yearfrom4 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @user_devise.career_monthfrom4 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center"><%= @user_devise.company4 %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s">住 所</td>
            <td class="bd-t-dt bd-r-s" colspan="5">( 〒 <%= @user_devise.postal %> )</td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @user_devise.career_yearfrom5 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @user_devise.career_monthfrom5 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center"><%= @user_devise.company5 %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-r-s bd-l-s" colspan="6" rowspan="2"><%= @user_devise.address %><%= @user_devise.address2 %><%= @user_devise.address3 %></td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @user_devise.career_yearfrom6 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @user_devise.career_monthfrom6 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center"><%= @user_devise.company6 %></td>
          </tr>
          <tr style="height:33.5px">
            <td></td>
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center"></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-s bd-l-s">電話</td>
            <td class="bd-t-s" colspan="2"></td>
            <td class="bd-t-s bd-l-s">携帯電話</td>
            <td class="bd-t-s bd-r-s" colspan="2"><%= @user_devise.number %></td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center"></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-s bd-l-s">E-mail</td>
            <td class="bd-t-s bd-r-s" colspan="5"><%= @user_devise.email %></td>
            <td></td>
            <td class="bd-t-s bd-l-s center">年</td>
            <td class="bd-t-s bd-l-dt center">月</td>
            <td class="bd-t-s bd-r-s bd-l-dt center">免許・資格</td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-s bd-l-s center">年</td>
            <td class="bd-t-s bd-l-dt center">月</td>
            <td class="bd-t-s bd-r-s bd-l-dt center" colspan="4">学歴</td>
            <td></td>
            <td class="bd-t-db bd-l-s center"><%= @qualification_year1 %></td>
            <td class="bd-t-db bd-l-dt center"><%= @qualification_month1 %></td>
            <td class="bd-t-db bd-r-s bd-l-dt"><%= @qualification1 %> <% if !@qualification1.empty? %>取得<% end %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"><%= @user_devise.educational_yearfrom1 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @user_devise.educational_monthfrom1 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt" colspan="4"><%= @user_devise.school1 %> <% if !@user_devise.school1.nil? %>入学<% end %> </td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @qualification_year2 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @qualification_month2 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt"><%= @qualification2 %> <% if !@qualification2.empty? %>取得<% end %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"><%= @user_devise.educational_yearto1 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @user_devise.educational_monthto1 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt" colspan="4"><%= @user_devise.school1 %> <% if !@user_devise.school1.nil? %>卒業<% end %> </td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @qualification_year3 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @qualification_month3 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt"><%= @qualification3 %> <% if !@qualification3.empty? %>取得<% end %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center" colspan="4"></td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @qualification_year4 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @qualification_month4 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt"><%= @qualification4 %> <% if !@qualification4.empty? %>取得<% end %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"><%= @user_devise.educational_yearfrom2 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @user_devise.educational_monthfrom2 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt" colspan="4"><%= @user_devise.school2 %> <% if !@user_devise.school2.nil? %>入学<% end %> </td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"><%= @qualification_year5 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @qualification_month5 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt"><%= @qualification5 %> <% if !@qualification5.empty? %>取得<% end %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"><%= @user_devise.educational_yearto2 %></td>
            <td class="bd-t-dt bd-l-dt center"><%= @user_devise.educational_monthto2 %></td>
            <td class="bd-t-dt bd-r-s bd-l-dt" colspan="4"><%= @user_devise.school2 %>  <% if !@user_devise.school2.nil? %>卒業<% end %> </td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt"></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center" colspan="4"></td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt"></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt center" colspan="4"></td>
            <td></td>
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt"></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt" colspan="4"></td>
            <td></td>
            <td class="bd-t-s bd-r-s bd-l-s" colspan="3">本人希望記入欄</td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt" colspan="4"></td>
            <td></td>
            <td class="bd-t-s bd-r-s bd-b-s bd-l-s" colspan="3" rowspan="3"><%= @hope %></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-l-s center"></td>
            <td class="bd-t-dt bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-l-dt" colspan="4"></td>
            <td></td>
          </tr>
          <tr style="height:33.5px">
            <td class="bd-t-dt bd-b-s bd-l-s center"></td>
            <td class="bd-t-dt bd-b-s bd-l-dt center"></td>
            <td class="bd-t-dt bd-r-s bd-b-s bd-l-dt" colspan="4"></td>
            <td></td>
          </tr>
        </tbody>
      </table>
    </page>
  </body>
</html>