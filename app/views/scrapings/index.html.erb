<section class="p-search">
<%= search_form_for @q, url: scrapings_search_path do |f| %>

  <%= f.select :status_cont, [['正社員', '正社員'], ['契約社員', '契約社員'], ['業務委託', '業務委託'], ['アルバイト', 'アルバイト']], {include_blank: '働き方・雇用形態'}, class: 'c-select__status' %>

  <%= f.search_field :name_cont_any, placeholder: "職種、業種、働き方など", class: "c-field__free" %>

  <%= f.search_field :location_cont_any, placeholder: "都道府県、市町村区、駅名", class: "c-field__prefectures" %>

  <%= f.submit '検索', class: "c-btn__search" %>
<% end %>
</section>

